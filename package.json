{
  "name": "fty-utils",
  "version": "1.0.0",
  "description": "Utilities to pack and unpack directories using FTY format",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "fty-pack": "./dist/fty-pack.js",
    "fty-unpack": "./dist/fty-unpack.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "node dist/test.js",
    "fty-pack": "fty-pack . -o package.fty.yaml --gitignore -i \".git,node_modules,dist,pnpm-lock.yaml\"",
    "fty-unpack": "fty-unpack package.fty.yaml -o ."
  },
  "keywords": [
    "fty",
    "yaml",
    "file-system",
    "pack",
    "unpack",
    "cli",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/minimatch": "^5.0.0",
    "@types/node": "^20.0.0",
    "fty-utils": "^1.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "js-yaml": "^4.0.0",
    "minimatch": "^9.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ]
}
